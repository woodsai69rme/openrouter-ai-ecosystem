# Stress Testing Environment Configuration
# Stress testing ports: 6006-6007

# Frontend Configuration
VITE_PORT=6006
VITE_API_URL=http://localhost:6007

# Backend Configuration
PORT=6007
NODE_ENV=stress

# API Configuration
API_BASE_URL=http://localhost:6007/api
CORS_ORIGIN=http://localhost:6006

# Stress Testing Specific Configuration
BATCH_SIZE=100
MAX_CONCURRENT_PROCESSES=50
REQUEST_TIMEOUT=60000
CONNECTION_POOL_SIZE=200
MEMORY_LIMIT=2048

# Rate Limiting (Maximum limits for stress testing)
RATE_LIMIT_WINDOW_MS=10000
RATE_LIMIT_MAX_REQUESTS=5000

# Performance Monitoring
ENABLE_METRICS=true
METRICS_INTERVAL=1000
ENABLE_PROFILING=true

# Error Handling
MAX_RETRIES=5
RETRY_DELAY=1000

# Logging
LOG_LEVEL=error
LO