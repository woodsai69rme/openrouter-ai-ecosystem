{
  "version": "1.0.0",
  "name": "OpenRouter Primary Configuration",
  "description": "Complete OpenRouter setup for zero-cost AI operations",
  
  "api": {
    "baseUrl": "https://openrouter.ai/api/v1",
    "key": "${OPENROUTER_API_KEY}",
    "timeout": 30000,
    "retries": 3,
    "rateLimit": {
      "requestsPerMinute": 60,
      "burstAllowance": 10
    }
  },

  "models": {
    "primary": [
      {
        "name": "openai/gpt-oss-20b:free",
        "priority": 1,
        "weight": 0.4,
        "specialization": "general",
        "maxLatency": 500,
        "costPerToken": 0.0
      },
      {
        "name": "z-ai/glm-4.5-air:free",
        "priority": 1,
        "weight": 0.3,
        "specialization": "multilingual",
        "maxLatency": 400,
        "costPerToken": 0.0
      },
      {
        "name": "google/gemini-2.0-flash-exp:free",
        "priority": 1,
        "weight": 0.3,
        "specialization": "reasoning",
        "maxLatency": 600,
        "costPerToken": 0.0
      }
    ],

    "specialized": [
      {
        "name": "qwen/qwen-2.5-coder-32b-instruct:free",
        "priority": 2,
        "specialization": "coding",
        "maxLatency": 700,
        "costPerToken": 0.0
      },
      {
        "name": "deepseek/deepseek-r1:free",
        "priority": 2,
        "specialization": "reasoning",
        "maxLatency": 800,
        "costPerToken": 0.0
      },
      {
        "name": "meta-llama/llama-3.3-70b-instruct:free",
        "priority": 2,
        "specialization": "analysis",
        "maxLatency": 900,
        "costPerToken": 0.0
      }
    ],

    "fallback": [
      {
        "name": "mistralai/mistral-small-3.2-24b-instruct:free",
        "priority": 3,
        "specialization": "general",
        "maxLatency": 1000,
        "costPerToken": 0.0
      },
      {
        "name": "qwen/qwq-32b:free",
        "priority": 3,
        "specialization": "reasoning",
        "maxLatency": 1000,
        "costPerToken": 0.0
      }
    ]
  },

  "routing": {
    "strategy": "performance",
    "loadBalancing": {
      "enabled": true,
      "algorithm": "weighted-round-robin",
      "healthCheck": {
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    "failover": {
      "enabled": true,
      "maxRetries": 2,
      "backoffStrategy": "exponential",
      "initialDelay": 1000
    }
  },

  "optimization": {
    "caching": {
      "enabled": true,
      "ttl": 300,
      "maxSize": "100MB"
    },
    "modelRotation": {
      "enabled": true,
      "rotationInterval": 60000,
      "preventRateLimits": true
    },
    "costTracking": {
      "enabled": true,
      "logLevel": "detailed",
      "alertThresholds": {
        "daily": 0.01,
        "monthly": 0.1
      }
    },
    "performance": {
      "monitorLatency": true,
      "targetLatency": 400,
      "adaptiveRouting": true
    }
  },

  "integration": {
    "claudeCode": {
      "enabled": true,
      "configPath": ".claude/config.json",
      "defaultModel": "openai/gpt-oss-20b:free"
    },
    "universalCLI": {
      "enabled": true,
      "primaryProvider": "openrouter",
      "fallbackProviders": ["gemini", "claude"]
    }
  },

  "monitoring": {
    "enabled": true,
    "dashboard": {
      "port": 3000,
      "updateInterval": 5000
    },
    "logging": {
      "level": "info",
      "file": "logs/openrouter.log",
      "maxSize": "10MB",
      "retention": "30d"
    },
    "metrics": {
      "collectUsage": true,
      "collectPerformance": true,
      "collectCosts": true,
      "exportFormat": "json"
    }
  },

  "security": {
    "apiKey": {
      "validation": true,
      "rotation": false,
      "encryption": "AES-256"
    },
    "rateLimiting": {
      "enabled": true,
      "global": "1000/hour",
      "perModel": "100/hour"
    }
  },

  "development": {
    "debugMode": false,
    "verboseLogging": false,
    "testMode": false,
    "mockResponses": false
  },

  "alerts": {
    "enabled": true,
    "channels": ["console", "log"],
    "conditions": [
      {
        "type": "high_latency",
        "threshold": 1000,
        "action": "switch_model"
      },
      {
        "type": "rate_limit_reached",
        "threshold": 0.9,
        "action": "rotate_model"
      },
      {
        "type": "cost_threshold",
        "threshold": 0.01,
        "action": "alert_admin"
      }
    ]
  },

  "metadata": {
    "created": "2025-01-10",
    "lastUpdated": "2025-01-10",
    "version": "1.0.0",
    "maintainer": "Claude Code AI System",
    "totalCost": "$0.00/month",
    "totalModels": 44,
    "expectedSavings": "$602.50/month"
  }
}