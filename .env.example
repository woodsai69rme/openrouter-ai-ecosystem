# Server Configuration
PORT=3000
NODE_ENV=development

# Database
POSTGRES_URL=postgresql://postgres:password@localhost:5432/ai_dev_center
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ai_dev_center
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# API Keys (Examples - Add your actual keys)
OPENROUTER_API_KEY=your-openrouter-api-key
CLAUDE_API_KEY=your-claude-api-key
GEMINI_API_KEY=your-gemini-api-key
GITHUB_TOKEN=your-github-token
TRAE_API_KEY=your-trae-api-key
BLACKBOX_API_KEY=your-blackbox-api-key

# External Services
OPENROUTER_API_URL=https://openrouter.ai/api/v1
CLAUDE_API_URL=https://api.anthropic.com
GEMINI_API_URL=https://generativelanguage.googleapis.com
TRAE_API_URL=https://api.trae.ai
BLACKBOX_API_URL=https://api.blackbox.ai
GITHUB_API_URL=https://api.github.com

# Security
BCRYPT_ROUNDS=12
SESSION_SECRET=your-session-secret-change-this
CORS_ORIGIN=http://localhost:3001

# Monitoring
ENABLE_MONITORING=true
LOG_LEVEL=info
METRICS_PORT=9090

# Features
ENABLE_WEBSITE_CLONING=true
ENABLE_AUTO_DISCOVERY=true
ENABLE_COST_OPTIMIZATION=true
ENABLE_MULTI_AI_COORDINATION=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100